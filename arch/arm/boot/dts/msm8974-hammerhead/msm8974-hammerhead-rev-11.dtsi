/include/ "msm8974-hammerhead.dtsi"
/include/ "msm8974-hammerhead-panel.dtsi"

&soc {
	i2c@f9924000 {
		synaptics_s3350@70 {
			status = "ok";
			synaptics,type = <1>;
		};
	};

	vibrator {
		vdd-supply = <&pm8941_l19>;
		use-vdd-supply;
	};

	gpio_keys {
		cover-switch {
			label = "cover-switch";
			gpios = <&msmgpio 144 0x01>;
			linux,input-type = <5>;
			linux,code = <0>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};
};

&mdss_mdp {
	qcom,mdss-pref-prim-intf = "dsi";
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_jdi_command_ors>;
};

&sdhc_1 {
	qcom,cpu-affinity = "all_cores";
	qcom,cpu-dma-latency-us = <2 201 501>;
};
